#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
      int n,i,j,t;
      int a[1000],b[1000];
      scanf("%d\n",&n);
      for(i=0;i<=n-1;i++)
      {
        scanf("%d ",&a[i]);
      }
      for(i=n-1,j=0;i>=0;i--,j++)
      {
          b[j]=a[i];
      }
       for(i=0;i<=n-1;i++)
       {
           a[i]=b[i];
       }
       for(i=0;i<=n-1;i++)
       {
           printf("%d ",a[i]);
           }
       }
