#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    int a[6],i,j,n=0,p=0;
    for(i=0;i<=2;i++)
    {
    scanf("%d",&a[i]);
    scanf("\n");
    }
    for(j=3;j<=5;j++)
    scanf("%d",&a[j]);
    for(i=0;i<=2;i++)
    {
        for(j=3+i;j<=3+i;j++)
        {
           if(a[i]>a[j])
            {
              n=n+1;
            }
        }
    }
               printf("%d",n);
               for(i=0;i<=2;i++)
               {
                   for(j=3+i;j<=3+i;j++)
                   {
                      if(a[i]<a[j])
                      {
                          p=p+1;
                      }
                   }
               }
               printf(" %d",p);
               return(0);
             
} 

