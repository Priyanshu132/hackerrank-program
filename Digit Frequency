#include <math.h>
#include <stdlib.h>
int main()
{
    char *s;
    int i,p,r=0;
    int c[10]={0};
    s=(char*)malloc(1005*sizeof(char));
      scanf("%s",s);
        for(i=0;i<strlen(s);i++)
        {
            if(s[i]>='0'&&s[i]<='9')
            {
             c[s[i]-'0']++;
            }
        }
 for(i=0;i<10;i++)
 printf("%d ",c[i]);
}
