#include<stdio.h>
int main()
{
    int i,a[4];
    for(i=0;i<=3;i++)
    scanf("%d\n",&a[i]);
    if((a[0]>a[1])&&(a[0]>a[2])&&(a[0]>a[3]))
    printf("%d",a[0]);
    else
    {
        if((a[1]>a[2])&&(a[1]>a[3]))
        printf("%d",a[1]);
        else
          {
              if(a[2]>a[3])
              printf("%d",a[2]);
              else
              printf("%d",a[3]);
          }
    }
}

