#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    int a,i,b,c=0;
    scanf("%d\n",&a);
    for(i=1;i<=a;i++)
    {
      scanf(" %d",&b);
      c=c+b;
    }
     printf("%d",c);
     return(0);
}

